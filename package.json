{
  "name": "blue-harvest-special-edition",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev --turbopack",
    "format": "prettier --write './**/*.{js,jsx,mjs,cjs,ts,tsx,json}' ",
    "lint": "next lint",
    "start": "next start",
